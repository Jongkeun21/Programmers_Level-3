    "def solution(n) :\n",
    "    \n",
    "    dic = {1: [0]}\n",
    "\n",
    "    def origami(count) :\n",
    "        if count not in dic :\n",
    "            before = origami(count-1)\n",
    "            dic[count] = before + [0] + [(element+1)%2 for element in before[::-1]]\n",
    "\n",
    "        return dic[count]\n",
    "    \n",
    "    return origami(n)"
   ]
  }
