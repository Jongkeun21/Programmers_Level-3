    "def solution(N) :\n",
    "    answer = 0\n",
    "    \n",
    "    dic = {1:4, 2:6}\n",
    "    \n",
    "    for i in range(N+1) :\n",
    "        if i > 2 :\n",
    "            dic[i] = dic[i-2]+dic[i-1]\n",
    "    \n",
    "    answer = dic[N]\n",
    "    \n",
    "    return answer"
   ]
  }
